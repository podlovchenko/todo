{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "extends": [
    "airbnb"
  ],
  "rules":{
    "linebreak-style": 0,
    "jsx-a11y/href-no-hash": 0,
    "import/extensions": 0,
    "no-console": 0,
    "no-use-before-define": 0,
    "no-return-assign": 0,
    "class-methods-use-this": 0,
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0
  }
}
